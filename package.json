{
  "name": "utils-mongodb",
  "version": "0.0.2",
  "main": "dist/index.js",
  "scripts": {
    "rmDist": "node -e 'fs.rmSync(`dist`, {recursive:true, force:true})'",
    "rmNodeModules": "node -e 'fs.rmSync(`node_modules`, {recursive:true, force:true})'",
    "rmPackageLock": "node -e 'fs.rmSync(`package-lock.json`, {recursive:true, force:true})'",
    "cleanup": "npm run rmDist && npm run rmNodeModules && npm run rmPackageLock",
    "build": "npm run cleanup && npm install && tsc",
    "tsc": "tsc"
  },
  "author": "sawara7",
  "license": "MIT",
  "bundleDependencies": [],
  "devDependencies": {
    "typescript": "^5.5.3",
    "@types/node": "^20.14.11"
  },
  "dependencies": {
    "mongodb": "^6.1.0"
  }
}
